{
	"name": "wx-command",
	"displayName": "wx-command",
	"description": "微信小程序文件助手",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.wxPage",
		"onCommand:extension.wxComponents"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.wxPage",
				"title": "wx page"
			},
			{
				"command": "extension.wxComponents",
				"title": "wx components"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.wxPage"
				},
				{
					"command": "extension.wxComponents"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.19.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
